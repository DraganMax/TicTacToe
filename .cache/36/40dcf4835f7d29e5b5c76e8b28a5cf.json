 && this.getWinner() === '-';\r\n  }\r\n\r\n  onClick(i: number): void {\r\n    if (this.getWinner() !== '-') {\r\n      return;\r\n    }\r\n    if (this.cells[i] !== '-') {\r\n      return\r\n    }\r\n    this.cells[i] = this.getTurn();\r\n  }\r\n\r\n  restart(): void {\r\n    this.cells = [\"-\", \"-\", \"-\", \"-\", \"-\", \"-\", \"-\", \"-\", \"-\"];\r\n    console.log(\"Restarting\");\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"61cc69aaa4a30b4c6418df2d648ebc82","cacheData":{"env":{}}}